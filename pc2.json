{
    "name": "System run log and file handling",
    "version": "1.0.0",
    "description": "This is an FNS Package",
    "deployment_resources_size": "small",
    "database_resources_size": "small",
    "settings": {
        "some_service_url": "http://localhost:5678",
        "check_interval_seconds": "10"
    },
    "permissions": {
        "logs": {
            "read": {
                "name": "Read Logs",
                "description": "Allows members to read logs",
                "assignable": "true",
                "aggregate_limitations": []
            }
        },
        "packages": {
            "read": {
                "name": "Read Packages",
                "description": "Allows members to read packages",
                "assignable": "true",
                "aggregate_limitations": []
            }
        },
        "files": {
            "list": {
                "name": "List Files",
                "description": "Allows members to list files",
                "assignable": "true",
                "aggregate_limitations": []
            },
            "download": {
                "name": "Download Files",
                "description": "Allows members to download files",
                "assignable": "true",
                "aggregate_limitations": []
            },
            "upload": {
                "name": "Upload Files",
                "description": "Allows members to upload files",
                "assignable": "true",
                "aggregate_limitations": []
            },
            "delete": {
                "name": "Delete Files",
                "description": "Allows members to delete files",
                "assignable": "true",
                "aggregate_limitations": []
            },
            "batchDelete": {
                "name": "Delete multiple Files",
                "description": "Allows members to delete multiple files in one go",
                "assignable": "true",
                "aggregate_limitations": []
            }
        }
    },
    "required_permissions": {
        "logs": [
            "read"
        ]
    },
    "deploymentConfiguration": {
        "views": {
            "name": "system-run-log-and-file-handling"
        }
    }
}